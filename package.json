  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "core-js": "3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express-session": "^1.16.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "querystring": "^0.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "node-sass": "^4.12.0",
    "spotify-web-api-node": "^4.0.0"
  },
  "proxy": "http://localhost:3001/",
  "scripts": {
    "server": "babel-node src/server",
    "dev": "concurrently \"yarn server\" \"yarn start\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix --ext .js src",
    "heroku-postbuild": "npm run build && concurrently \"npm run server\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
